{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:devnet": "vue-cli-service serve --mode devnet",
    "serve:mainnet": "vue-cli-service serve --mode mainnet",
    "build": "vue-cli-service build",
    "build:devnet": "vue-cli-service build --mode devnet",
    "build:mainnet": "vue-cli-service build --mode mainnet",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.17.6",
    "@metaplex-foundation/js-plugin-nft-storage": "^0.1.1",
    "@project-serum/anchor": "^0.25.0",
    "@solana/spl-token": "^0.3.6",
    "@solana/wallet-adapter-wallets": "^0.19.5",
    "@solana/web3.js": "^1.69.0",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "core-js": "^3.8.3",
    "postcss": "^8.4.18",
    "solana-wallets-vue": "^0.5.2",
    "tailwindcss": "^3.2.2",
    "vue": "^3.2.13",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.12.16",
    "@tailwindcss/forms": "^0.5.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/script-setup-uses-vars": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
